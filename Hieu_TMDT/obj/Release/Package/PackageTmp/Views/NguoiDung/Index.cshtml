@model IEnumerable<Hieu_TMDT.NGUOI_DUNG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MyAdmin.cshtml";
}


<div class="breadcrumbs ace-save-state" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="/Home/AdminPage">Home</a>
        </li>
        <li class="active">Quản lí Tài Khoản</li>
    </ul>
    <div class="nav-search" id="nav-search">
        <form class="form-search">
            <span class="input-icon">
                <input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
                <i class="ace-icon fa fa-search nav-search-icon"></i>
            </span>
        </form>
    </div><!-- /.nav-search -->
</div>
<h2>Danh Sách Tài Khoản</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PASSWORD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDQH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.XACTHUC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BARCODE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMAIL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HO_TEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIA_CHI)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PASSWORD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IDQH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.XACTHUC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BARCODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EMAIL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HO_TEN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DIA_CHI)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.USERNAME }) |
                @Html.ActionLink("Details", "Details", new { id = item.USERNAME }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.USERNAME })
            </td>
        </tr>
    }

</table>
